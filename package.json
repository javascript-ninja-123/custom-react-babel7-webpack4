  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-test))"
    ],
    "cacheDirectory": "./node_modules/.cache/jest",
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest",
      "^.+\\.scss$": "<rootDir>/node_modules/jest-css-modules",
      "^[./a-zA-Z0-9$_-]+\\.(bmp|gif|jpg|jpeg|png|psd|svg|webp)$": "<rootDir>/src/mediaFileTransformer.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "setupTestFrameworkScriptFile": "./src/setupTest.js",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ]
