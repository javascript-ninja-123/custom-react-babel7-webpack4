    "start-dev-all": "concurrently \"npm run start-dev\" \"npm run storybook\"",
    "build-dev": "rm -rf build && webpack --config config/webpack.config.dev.js",
    "build-prod": "rm -rf build && webpack --config config/webpack.config.prod.js",
